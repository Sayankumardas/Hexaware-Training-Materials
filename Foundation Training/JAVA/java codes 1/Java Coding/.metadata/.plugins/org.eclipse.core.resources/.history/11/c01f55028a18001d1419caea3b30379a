package com.example.basic.comments;

public class Comments {
	
	public static void main(String[] args) {
		
		//This is single line commenting
		
		/*
		 * Multiple line commenting
		 * 
		 * */
		
		
	}
	
	
	/**
	 * @param a
	 * @param b
	 * @return
	 */
